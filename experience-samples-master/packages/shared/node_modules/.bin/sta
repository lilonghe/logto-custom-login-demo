#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/workspace/test/experience-samples-master/node_modules/.pnpm/swagger-typescript-api@13.0.21/node_modules/swagger-typescript-api/dist/node_modules:/mnt/d/workspace/test/experience-samples-master/node_modules/.pnpm/swagger-typescript-api@13.0.21/node_modules/swagger-typescript-api/node_modules:/mnt/d/workspace/test/experience-samples-master/node_modules/.pnpm/swagger-typescript-api@13.0.21/node_modules:/mnt/d/workspace/test/experience-samples-master/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/workspace/test/experience-samples-master/node_modules/.pnpm/swagger-typescript-api@13.0.21/node_modules/swagger-typescript-api/dist/node_modules:/mnt/d/workspace/test/experience-samples-master/node_modules/.pnpm/swagger-typescript-api@13.0.21/node_modules/swagger-typescript-api/node_modules:/mnt/d/workspace/test/experience-samples-master/node_modules/.pnpm/swagger-typescript-api@13.0.21/node_modules:/mnt/d/workspace/test/experience-samples-master/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../swagger-typescript-api/dist/cli.js" "$@"
else
  exec node  "$basedir/../swagger-typescript-api/dist/cli.js" "$@"
fi
